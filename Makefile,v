		$(LD) -m -o $(CODE_SEG) -r $(NISHIDA_OBJS) $(KAWAGOE_OBJS) $(YAJIMA_OBJS) $(TANIMOTO_OBJS) $(IWAMOTO_OBJS) $(IWAWAKI_OBJS) $(AUDIO_OBJS) $(LD_FLAGS) > link.map
