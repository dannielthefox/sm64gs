map/back/mainmap_backscrn.o: map/back/mainmap_backscrn.c
	press map/back/mainmap_backscrn $(SEGMENT_BACK) > symbol.out
	convszp symbol.out map/back/mainmap_backscrn > null
	cc $(CC_FLAGS) -o map/back/mainmap_backscrn.o  map/back/mainmap_backscrn.s

