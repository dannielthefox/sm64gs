#################################################################################
#																				#
#	making mario																#
#																				#
#################################################################################

LD		  = /usr/lib/PR/nld
CC_FLAGS  = -Wab,-r4300_mul -O2 -float -mips2 -xansi -signed -fullwarn -prototypes -Xcpluscomm -non_shared -I/usr/include/PR -G 0 -c
#LD_FLAGS  = $(MKDEPOPT) -nostdlib -L/usr/lib -L/usr/lib/PR -lultra_d
LD_FLAGS  = $(MKDEPOPT) -nostdlib -L/usr/lib -L/usr/lib/PR -lultra_rom

CODE_SEG  = code.out
ULIB_SEG  = ulib.out


rom $(APP):	spec $(CODE_SEG) $(ULIB_SEG) $(DEMO_OBJS) $(SHAPE) $(DATA) $(ANIME) $(BACKSCRN) $(SEQUENCE) $(MAP) $(MAPTEXTURE) $(STAGE)
#		makerom -s 64 -f 0xff -m -r rom spec > rom.map 
		makerom -m -r rom spec > rom.map
		copymask rom mask
		makemask mask

$(CODE_SEG): $(BROADON_OBJS) $(NISHIDA_OBJS) $(YAJIMA_OBJS) $(KAWAGOE_OBJS) $(TANIMOTO_OBJS) $(IWAMOTO_OBJS) $(IWAWAKI_OBJS) $(AUDIO_OBJS)
		$(LD) -m -o $(CODE_SEG) -r $(BROADON_OBJS) $(NISHIDA_OBJS) $(KAWAGOE_OBJS) $(YAJIMA_OBJS) $(TANIMOTO_OBJS) $(IWAMOTO_OBJS) $(IWAWAKI_OBJS) $(AUDIO_OBJS) $(LD_FLAGS) > link.map

$(ULIB_SEG): $(USRLIB_OBJS)
		$(LD) -m -o $(ULIB_SEG) -r $(USRLIB_OBJS) $(MKDEPORT) -nostdlib  > ulib.map


